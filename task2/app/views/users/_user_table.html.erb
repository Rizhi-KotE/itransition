<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>ID</th>
            <th>Name</th>
            <th>E-mail</th>
        </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <% if user.access_locked? %>
        <tr class="danger">
      <% else %>
        <tr>
      <% end %>
        <td>
          <% if not user.id == @user.id %>
            <%= check_box_tag(name="user_id[]", value =  user.id) %>
          <% end %>
        </td>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
      </tr>
      <% end %>
    </tbody>
</table>
